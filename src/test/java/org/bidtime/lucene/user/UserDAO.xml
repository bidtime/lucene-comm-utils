<?xml version="1.0" encoding="utf-8"?>

<classes>

<class package="" name="User" table="u_user">
	<id name="userId" column="userId" type="int" length="10" not-null="true" generator="identity" store="yes" index="yes" tokenized="yes">
	<property name="loginMobile" column="loginMobile" type="varchar" length="20" not-null="true" store="yes" index="yes" tokenized="yes"/>
	<property name="pwd" column="pwd" type="varchar" length="96" not-null="true" store="yes" index="yes" tokenized="yes"/>
	<property name="userName" column="userName" type="varchar" length="20" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="sex" column="sex" type="tinyint" length="2" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="provinceId" column="provinceId" type="int" length="11" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="cityId" column="cityId" type="int" length="11" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="countyId" column="countyId" type="int" length="11" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="code" column="code" type="varchar" length="100" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="sign" column="sign" type="varchar" length="100" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="imageUrl" column="imageUrl" type="varchar" length="100" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="contactphone" column="contactphone" type="varchar" length="20" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="userType" column="userType" type="int" length="4" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="idCardUrl2" column="idCardUrl2" type="varchar" length="255" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="idCardUrl" column="idCardUrl" type="varchar" length="255" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="memo" column="memo" type="varchar" length="255" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="state" column="state" type="tinyint" length="4" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="auth" column="auth" type="tinyint" length="4" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="createTime" column="createTime" type="datetime" not-null="false" store="yes" index="yes" tokenized="yes"/>
	<property name="updateTime" column="updateTime" type="datetime" not-null="false" store="yes" index="yes" tokenized="yes">
</class>

<sql>
	<id name="getFullSql">
	<![CDATA[
	SELECT
	  userId,
	  loginMobile,
	  pwd,
	  userName,
	  sex,
	  sign,
	  code,
	  provinceId,
	  cityId,
	  countyId,
	  imageUrl,
	  contactphone,
	  userType,
	  memo,
	  idCardUrl,
	  idCardUrl2,
	  auth,
	  state,
	  createTime,
	  updateTime
	FROM
		u_user
	WHERE TRUE=TRUE
	  << AND userId = #userId# >>
	  << AND loginMobile like #loginMobile# >>
	  << AND loginMobile = #loginMobile_equal# >>
	  << AND userName like #userName# >>
	  << AND sex = #sex# >>
	  << AND provinceId = #provinceId# >>
	  << AND cityId = #cityId# >>
	  << AND countyId = #countyId# >>
	  << AND imageUrl = #imageUrl# >>
	  << AND contactphone = #contactphone# >>
	  << AND userType = #userType# >>
	  << AND state = #state# >>
	  order by userId desc
	]]>
	</id>

</sql>

</classes>
